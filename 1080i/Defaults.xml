<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <default type="label">
        <height>75</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>size33</font>
        <textcolor>main</textcolor>
        <focusedcolor>main</focusedcolor>
        <selectedcolor>orange</selectedcolor>
        <disabledcolor>dimgray</disabledcolor>
        <shadowcolor>inverted</shadowcolor>
        <scroll>true</scroll>
        <scrollspeed>50</scrollspeed>
        <scrollsuffix> • </scrollsuffix>
    </default>
    <default type="fadelabel">
        <height>75</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>size33</font>
        <textoffsetx>30</textoffsetx>
        <textcolor>main</textcolor>
        <focusedcolor>main</focusedcolor>
        <selectedcolor>orange</selectedcolor>
        <disabledcolor>dimgray</disabledcolor>
        <scrollout>false</scrollout>
        <pauseatend>3000</pauseatend>
        <scrollsuffix> •  • </scrollsuffix>
    </default>
    <default type="textbox">
        <font>size28</font>
        <textcolor>main</textcolor>
        <focusedcolor>main</focusedcolor>
        <selectedcolor>orange</selectedcolor>
        <disabledcolor>dimgray</disabledcolor>
        <pagecontrol>61</pagecontrol>
        <pulseonselect>false</pulseonselect>
        <autoscroll delay="10000" time="3000" repeat="10000">true</autoscroll>
    </default>
    <default type="image">
        <align>center</align>
        <aligny>center</aligny>
        <aspectratio>stretch</aspectratio>
    </default>
    <default type="button">
        <width>390</width>
        <height>75</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>size33</font>
        <textoffsetx>30</textoffsetx>
        <textcolor>maindim</textcolor>
        <focusedcolor>main</focusedcolor>
        <selectedcolor>orange</selectedcolor>
        <disabledcolor>dimgray</disabledcolor>
        <invalidcolor>red</invalidcolor>
        <texturefocus colordiffuse="$VAR[diffuseselect]">images/buttonselect2.png</texturefocus>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="edit">
        <width>390</width>
        <height>75</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>size33</font>
        <textoffsetx>30</textoffsetx>
        <textcolor>maindim</textcolor>
        <focusedcolor>main</focusedcolor>
        <selectedcolor>main</selectedcolor>
        <disabledcolor>dimgray</disabledcolor>
        <invalidcolor>red</invalidcolor>
        <texturefocus border="8">images/textedit.png</texturefocus>
        <texturenofocus border="8" colordiffuse="99FFFFFF">images/textedit.png</texturenofocus>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="togglebutton">
        <width>450</width>
        <height>75</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>size33</font>
        <textoffsetx>30</textoffsetx>
        <textcolor>maindim</textcolor>
        <focusedcolor>main</focusedcolor>
        <selectedcolor>main</selectedcolor>
        <altselectedcolor>orange</altselectedcolor>
        <disabledcolor>dimgray</disabledcolor>
        <texturefocus colordiffuse="$VAR[diffuseselect]">images/buttonselect2.png</texturefocus>
        <alttexturefocus colordiffuse="$VAR[diffuseselect]">images/buttonselect2.png</alttexturefocus>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="radiobutton">
        <height>75</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>size33</font>
        <textcolor>maindim</textcolor>
        <focusedcolor>main</focusedcolor>
        <selectedcolor>main</selectedcolor>
        <disabledcolor>dimgray</disabledcolor>
        <radiowidth>46</radiowidth>
        <radioheight>46</radioheight>
        <texturefocus colordiffuse="$VAR[diffuseselect]">images/buttonselect1.png</texturefocus>
        <textureradiofocus aspectratio="keep">images/RadioON.png</textureradiofocus>
        <textureradionofocus aspectratio="keep">images/RadioOFF.png</textureradionofocus>
        <textoffsetx>30</textoffsetx>
        <!--<radioleft>262</radioleft>-->
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrol">
        <textureup>images/ArrowUpNF.png</textureup>
        <textureupfocus>images/ArrowUpFO.png</textureupfocus>
        <texturedown>images/ArrowDownNF.png</texturedown>
        <texturedownfocus>images/ArrowDownFO.png</texturedownfocus>
        <texturefocus colordiffuse="$VAR[diffuseselect]">images/buttonselect2.png</texturefocus>
        <spinwidth>18</spinwidth>
        <spinheight>18</spinheight>
        <font>size33</font>
        <textcolor>maindim</textcolor>
        <focusedcolor>main</focusedcolor>
        <selectedcolor>main</selectedcolor>
        <disabledcolor>dimgray</disabledcolor>
        <textoffsetx>30</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrolex">
        <width>450</width>
        <height>75</height>
        <font>size33</font>
        <textureup>images/ArrowUpNF.png</textureup>
        <textureupfocus>images/ArrowUpFO.png</textureupfocus>
        <texturedown>images/ArrowDownNF.png</texturedown>
        <texturedownfocus>images/ArrowDownFO.png</texturedownfocus>
        <texturefocus colordiffuse="$VAR[diffuseselect]">images/buttonselect2.png</texturefocus>
        <spinwidth>18</spinwidth>
        <spinheight>18</spinheight>
        <textcolor>maindim</textcolor>
        <focusedcolor>main</focusedcolor>
        <selectedcolor>main</selectedcolor>
        <disabledcolor>dimgray</disabledcolor>
        <textoffsetx>30</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="slider" description="Seek Slider">
        <width>711</width>
        <height>36</height>
    </default>
    <default type="sliderex" description="Default Slider">
        <left>795</left>
        <top>225</top>
        <height>75</height>
        <width>450</width>
        <sliderwidth>150</sliderwidth>
        <sliderheight>45</sliderheight>
        <texturesliderbar>images/slider_bg.png</texturesliderbar>
        <textureslidernib>images/slider_nib_nf.png</textureslidernib>
        <textureslidernibfocus>images/slider_nib.png</textureslidernibfocus>
        <texturefocus colordiffuse="$VAR[diffuseselect]">images/buttonselect2.png</texturefocus>
        <texturenofocus>-</texturenofocus>
        <font>size33</font>
        <aligny>center</aligny>
        <textcolor>maindim</textcolor>
        <focusedcolor>main</focusedcolor>
        <selectedcolor>main</selectedcolor>
        <disabledcolor>dimgray</disabledcolor>
        <textoffsetx>20</textoffsetx>
    </default>
    <default type="scrollbar">
        <width>9</width>
        <orientation>vertical</orientation>
        <texturesliderbackground border="4">Scrollbar/ScrollbarBG.png</texturesliderbackground>
        <texturesliderbar border="4">Scrollbar/ScrollerNF.png</texturesliderbar>
        <texturesliderbarfocus border="4">Scrollbar/ScrollerFO.png</texturesliderbarfocus>
        <pulseonselect>false</pulseonselect>
        <showonepage>false</showonepage>
    </default>
    <constant name="settingsheight">60</constant>
</includes>
